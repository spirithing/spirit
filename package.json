{
  "name": "spirit",
  "description": "Spirit is a startup ai app.",
  "version": "0.0.1",
  "packageManager": "pnpm@8.12.1",
  "main": "out/main/index.js",
  "imports": {
    "#main/*": "./main/*",
    "#preload/*": "./preload/*",
    "#renderer/*": "./renderer/*"
  },
  "type": "module",
  "scripts": {
    "prepare": "rm -rf node_modules/electron/dist && node node_modules/electron/install.js",
    "prebuild": "electron-vite build",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@larksuiteoapi/node-sdk": "^1.46.0",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@shikitor/core": "^0.0.9",
    "@shikitor/react": "^0.1.5",
    "@sinclair/typebox": "^0.34.33",
    "@uiw/react-color": "^2.3.0",
    "foxact": "^0.2.33",
    "framer-motion": "^11.2.10",
    "get-windows": "^9.1.1",
    "i18next": "^23.11.5",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "mathjs": "^14.4.0",
    "meshgrad": "^0.0.15",
    "nzh": "^1.0.14",
    "ollama": "^0.5.14",
    "open": "^10.1.0",
    "openai": "^4.89.0",
    "react-i18next": "^14.1.2",
    "shiki": "^1.6.2",
    "simple-plist": "^1.3.1",
    "swr": "^2.2.5",
    "tdesign-icons-react": "^0.3.2",
    "tdesign-react": "^1.7.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@shikijs/markdown-it": "^1.6.0",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^18.19.33",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.3.0",
    "dotenv": "^16.4.5",
    "dprint": "^0.46.1",
    "electron": "^35.1.4",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "hastscript": "^9.0.0",
    "iconutil": "^1.0.2",
    "jotai": "^2.8.2",
    "prettier": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.2",
    "sips": "^1.0.3",
    "typescript": "^5.4.5",
    "vite": "^5.0.12",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "electron-vite@2.2.0": "patches/electron-vite@2.2.0.patch"
    }
  }
}
