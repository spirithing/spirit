{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "main/*.share.d.ts",
    "sharing/*.d.ts",
    "preload/*.d.ts",
    "renderer/**/*",
    "renderer/env.d.ts",
    "renderer/**/*.tsx",
    "renderer/.core/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "moduleResolution": "Bundler"
  }
}
