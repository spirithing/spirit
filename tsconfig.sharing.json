{
  "include": [
    "sharing/**/*.ts"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "composite": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "types": [],
    "lib": [
      "ESNext",
      "ESNext.Disposable"
    ]
  }
}
